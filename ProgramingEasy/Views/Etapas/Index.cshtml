@model IEnumerable<EASY_PROGRAMING_PLATAFORM.Models.Etapas>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nivel)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nivel)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_etapa">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_etapa">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_etapa">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
